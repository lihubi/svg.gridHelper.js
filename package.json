{
  "name": "svg-gridhelper",
  "version": "0.0.7",
  "description": "An extension for svg.js which adds a grid helper to the svg",
  "type": "module",
  "main": "dist/svg.gridhelper.js",
  "module": "src/svg.gridhelper.js",
  "exports": {
    ".": {
      "import": {
        "types": "./svg.gridhelper.js.d.ts",
        "default": "./src/svg.gridhelper.js"
      },
      "require": {
        "types": "./svg.gridhelper.js.d.cts",
        "default": "./src/svg.gridhelper.js"
      },
      "browser": {
        "types": "./svg.gridhelper.js.d.ts",
        "default": "./src/svg.gridhelper.js"
      }
    }
  },
  "unpkg": "dist/svg.gridhelper.js",
  "jsdelivr": "dist/svg.gridhelper.js",
  "files": [
    "/dist",
    "/src",
    "/svg.gridhelper.js.d.ts",
    "/svg.gridhelper.js.d.cts"
  ],
  "keywords": [
    "svg.js",
    "gridhelper"
  ],
  "bugs": "https://github.com/svgdotjs/svg.gridhelper.js/issues",
  "license": "MIT",
  "typings": "./svg.gridhelper.js.d.ts",
  "author": {
    "name": "Li Kang"
  },
  "contributors": [
    {
      "name": "Li Kang"
    }
  ],
  "homepage": "https://github.com/svgdotjs/svg.gridhelper.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svgdotjs/svg.gridhelper.js.git"
  },
  "scripts": {
    "build": "npm run fix && vite build",
    "fix": "npx eslint --fix",
    "release": "standard-version"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  },
  "peerDependencies": {
    "@svgdotjs/svg.js": "^3.2.4"
  }
}
